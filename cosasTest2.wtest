import cosas2.*

describe "test de un par de cosas" {

  test "bateria antiaerea con misiles pesa 300" {
    bateriaAntiaerea.cargarMisiles()
    assert.equals(300, bateriaAntiaerea.peso())
  }

  test "embalaje con arena que pesa 200 tiene un peso par" {
    arena.peso(200)
    embalajeDeSeguridad.embalar(arena)
    assert.that(embalajeDeSeguridad.esPesoPar())
  }

}